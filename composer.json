{
    "name": "overblog/dataloader-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "DataLoader Symfony bundle implementation.",
    "keywords": ["dataLoader", "caching", "batching"],
    "authors": [
        {
            "name": "Overblog",
            "homepage": "http://www.over-blog.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Overblog\\DataLoaderBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config" : {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "require": {
        "php": "^5.5|^7.0",
        "overblog/dataloader-php": ">=0.4,<=0.5@dev",
        "symfony/framework-bundle": "^2.7|^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.1|^5.5",
        "react/promise": "^2.5",
        "symfony/dependency-injection": "^2.7|^3.1",
        "symfony/phpunit-bridge": "^2.7|^3.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
