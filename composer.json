{
    "name": "overblog/dataloader-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "DataLoader Symfony bundle implementation.",
    "keywords": ["dataLoader", "caching", "batching"],
    "authors": [
        {
            "name": "Overblog",
            "homepage": "http://www.over-blog.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Overblog\\DataLoaderBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config" : {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "require": {
        "php": ">=7.2",
        "overblog/dataloader-php": "^0.5.0 || ^0.6.0",
        "symfony/dependency-injection": "^4.3 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.1 || ^5.5 || ^6.5 || ^7.5 || ^8.5",
        "react/promise": "^2.5",
        "symfony/dependency-injection": "^4.3 || ^5.0",
        "symfony/framework-bundle": "^4.3 || ^5.0",
        "symfony/phpunit-bridge": "^4.3 || ^5.0",
        "symfony/yaml": "^4.3 || ^5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    }
}
